
<%= simple_form_for(@nf21) do |f| %>
  <%= f.error_notification %>
  <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

  <div class="form-inputs">
    <%= f.input :emissao %>
    <%= f.input :date, %>
    <%= f.input :numero %>
    <%= f.input :integer, %>
    <%= f.input :valor %>
    <%= f.input :decimal, %>
    <%= f.input :cadastro %>
    <%= f.input :text, %>
    <%= f.input :mestre %>
    <%= f.input :text %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
