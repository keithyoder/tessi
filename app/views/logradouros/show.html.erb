<div class="card">
  <div class="card-header">
    <h3><%= index_button(@logradouro.bairro) %>
    <%= edit_button(edit_logradouro_path(@logradouro)) %> Logradouro</h3>
  </div>
  <div class="card-body">
    <p>
      <strong>Nome:</strong>
      <%= @logradouro.nome %>
    </p>

    <p>
      <strong>Bairro:</strong>
      <%= @logradouro.bairro.nome_cidade_uf %>
    </p>

    <p>
      <strong>CEP:</strong>
      <%= @logradouro.cep %>
    </p>
  </div>
</div>
